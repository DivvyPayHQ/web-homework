{
  "name": "@divvy/react-challenge-web",
  "main": "index.js",
  "scripts": {
    "boot": "yarn --ignore-engines",
    "build": "webpack -d",
    "reset": "rm -rf node_modules 2> /dev/null && yarn boot",
    "start": "webpack-dev-server -d --hot",
    "test": "jest --coverage"
  },
  "resolutions": {
    "elliptic": " 6.5.3",
    "websocket-extensions": "0.1.4",
    "handlebars": "4.7.6",
