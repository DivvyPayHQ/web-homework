{
  "name": "@divvy/react-challenge-web",
  "main": "index.js",
  "scripts": {
    "build": "webpack -d",
    "start": "webpack-dev-server -d --hot",
    "test": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./test.setup.js"
    ],
    "setupFilesAfterEnv": [
      "./jest.config.framework.js"
    ],
    "globals": {
      "__DivvyEnvironment": true
    },
    "testRegex": ".+\\.tests?\\.jsx?$",
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/assetMock.js"
    },
    "snapshotSerializers": [
      "@emotion/jest/serializer"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".jsx?$": "babel-jest"
    }
