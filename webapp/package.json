    "snapshotSerializers": [
      "jest-emotion/serializer"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".jsx?$": "babel-jest"
    }
